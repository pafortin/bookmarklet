javascript:(function() {    var companiesInput = prompt("Enter your companies, separated by commas (up to 50)");    var titlesInput = prompt("Enter your job titles, separated by commas");    var companies = companiesInput.split(",");    var titles = titlesInput.split(",");    var original_url = "https://www.linkedin.com/sales/search/people?query=(recentSearchParam%3A(id%3A2590758754%2CdoLogHistory%3Atrue)%2Cfilters%3AList((type%3ACURRENT_COMPANY%2Cvalues%3AList((text%3AAAAAA%2CselectionType%3AINCLUDED)%2C(text%3ABBBBB%2520CCCCC%2CselectionType%3AINCLUDED)))%2C(type%3AREGION%2Cvalues%3AList((id%3A105015875%2Ctext%3AFrance%2CselectionType%3AINCLUDED)))%2C(type%3ACURRENT_TITLE%2Cvalues%3AList((text%3AFFFFF%2CselectionType%3AINCLUDED)%2C(text%3AGGGGG%2520HHHHH%2CselectionType%3AINCLUDED)))))&sessionId=Ny6jUkTdQ0qh2HciS%2Fq36A%3D%3D&viewAllFilters=true";        var outputWindow = window.open("", "OutputWindow", "width=800,height=600,scrollbars=1,resizable=1");    outputWindow.document.write("<html><head><title>Generated URLs</title></head><body><textarea id=%27urls%27 style=%27width:100%;height:90%;%27></textarea><button id=%27copyButton%27 style=%27width:100%;%27>Copy URLs</button></body></html>");    for (var i = 0; i < Math.ceil(companies.length/50); i++) {        var currentCompanies = companies.slice(i*50, (i+1)*50);        var companiesUrlPart = currentCompanies.map(function(company) {            return `(text%3A${encodeURIComponent(company.trim())}%2CselectionType%3AINCLUDED)`;        }).join(",");                var urlWithCompanies = original_url.replace("(type%3ACURRENT_COMPANY%2Cvalues%3AList((text%3AAAAAA%2CselectionType%3AINCLUDED)%2C(text%3ABBBBB%2520CCCCC%2CselectionType%3AINCLUDED)))", `(type%3ACURRENT_COMPANY%2Cvalues%3AList(${companiesUrlPart}))`);                var titlesUrlPart = titles.map(function(title) {            return `(text%3A${encodeURIComponent(title.trim())}%2CselectionType%3AINCLUDED)`;        }).join(",");                var finalUrl = urlWithCompanies.replace("(type%3ACURRENT_TITLE%2Cvalues%3AList((text%3AFFFFF%2CselectionType%3AINCLUDED)%2C(text%3AGGGGG%2520HHHHH%2CselectionType%3AINCLUDED)))", `(type%3ACURRENT_TITLE%2Cvalues%3AList(${titlesUrlPart}))`);        outputWindow.document.getElementById(%27urls%27).value += finalUrl + "\n";    }        outputWindow.document.getElementById(%27copyButton%27).addEventListener(%27click%27, function() {        outputWindow.document.getElementById(%27urls%27).select();        outputWindow.document.execCommand(%27copy%27);    });})()
